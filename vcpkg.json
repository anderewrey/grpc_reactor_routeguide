{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "grpc-reactor-routeguide",
  "version": "0.1.0",
  "description": "gRPC RouteGuide example demonstrating Reactor Callbacks API with modern C++20",
  "dependencies": [
    "grpc",
    "spdlog",
    "gflags",
    "glaze",
    "eventloop",
    "gtest"
  ],
  "overrides": [
    { "name": "spdlog", "version": "1.15.3" },
    { "name": "gflags", "version": "2.2.2" },
    { "name": "gtest", "version": "1.17.0" },
    { "name": "glaze", "version": "2.9.2" }
  ]
}
